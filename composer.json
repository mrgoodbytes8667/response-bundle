{
    "name": "mrgoodbytes8667/response-bundle",
    "type": "symfony-bundle",
    "license": "CC-BY-NC-4.0",
    "description": "Symfony bundle that wraps response-common and provides shared enums for child response-bundles",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.8",
        "mrgoodbytes8667/enum-serializer-bundle": "^1.3.1",
        "mrgoodbytes8667/http-client-common": "^0.2.2",
        "symfony/config": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/security-core": "^5.3",
        "symfony/security-csrf": "^5.3",
        "symfony/security-http": "^5.3",
        "symfony/serializer": "^5.3",
        "symfony/string": "^5.3",
        "symfony/uid": "^5.3"
    },
    "require-dev": {
        "mrgoodbytes8667/string-mask-bundle": "^0.0.1",
        "mrgoodbytes8667/test-common": ">=0.0.14",
        "mrgoodbytes8667/test-common-faker": "^0.3.3",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^5.3"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "suggest": {
        "mrgoodbytes8667/string-mask-bundle": "Mask tokens when displaying them"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bytes\\ResponseBundle\\": "src/",
            "Bytes\\ResponseBundle\\Tests\\": "Tests/"
        }
    },
    "conflict": {
        "phpunit/phpunit": "<9.5",
        "symfony/http-client": "<5.2.1"
    }
}
